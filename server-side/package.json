{
  "private": true,
  "name": "server-side",
  "version": "1.0.0",
  "description": "Doccure",
  "main": "dist/index.js",
  "scripts": {
    "clean:dist": "rimraf dist/*",
    "clean:migrations": "rimraf migrations/*.js",
    "build:migrations": "yarn clean:migrations && tsc --emitDecoratorMetadata true --experimentalDecorators true --outDir migrations migrations/*.ts",
    "migrations:create": "typeorm migration:create --dir migrations --name",
    "migrations:run": "yarn build:migrations && typeorm migration:run",
    "migrations:revert": "yarn build:migrations && typeorm migration:revert",
    "migrations:run:default": "yarn migrations:run --connection",
    "migrations:revert:default": "yarn migrations:revert --connection default",
    "mup": "yarn migrations:run:default",
    "mdn": "yarn migrations:revert:default",
    "mup:win": "yarn clean:migrations && npx tsc --emitDecoratorMetadata true --experimentalDecorators true --outDir migrations migrations/*.ts && npx typeorm migration:run --connection default",
    "mdn:win": "yarn clean:migrations && npx tsc --emitDecoratorMetadata true --experimentalDecorators true --outDir migrations migrations/*.ts && npx typeorm migration:revert --connection default",
    "lint:once": "eslint --ext .ts src",
    "lint:fix": "yarn lint:once --fix",
    "fix": "yarn lint:fix",
    "lint": "esw --watch --ext .ts src",
    "build": "yarn clean:dist && tsc",
    "start:tsclint": "yarn clean:dist && concurrently --kill-others --names lint,tsc,nodemon --prefix-colors green,blue,yellow \"yarn lint\" \"tsc --preserveWatchOutput --watch\"",
    "start": "yarn start:tsclint \"sleep 8 && nodemon\""
  },
  "keywords": [],
  "author": "Yavar Guliyev",
  "license": "MIT",
  "dependencies": {
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.3",
    "apollo-server-plugin-response-cache": "^3.7.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "graphql": "^15.4.0",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.30",
    "typeorm-typedi-extensions": "^0.4.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/graphql-upload": "^8.0.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.23",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-watch": "^8.0.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.3"
  }
}
